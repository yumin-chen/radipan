{
  "name": "radipan",
  "version": "0.0.8",
  "description": "React-compatible static CSS extraction based on `css` prop.",
  "author": "Yumin Chen <yumin@chen.so>",
  "type": "module",
  "main": "index.js",
  "module": "dist/radipan.js",
  "types": "dist/radipan.d.ts",
  "keywords": [
    "css",
    "static",
    "extraction"
  ],
  "bin": "dist/bin.js" ,
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "panda codegen",
    "build": "npm run prepare && rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yumin-chen/radipan.git"
  },
  "bugs": {
    "url": "https://github.com/yumin-chen/radipan/issues"
  },
  "homepage": "https://github.com/yumin-chen/radipan#readme",
  "license": "MIT",
  "files": [
    "index.js",
    "index.ts",
    "config.js",
    "config.ts",
    "tags.js",
    "tags.ts",
    "design-system.js",
    "design-system.ts",
    "dist",
    "extractor.tsconfig.json",
    "styles.css"
  ],
  "dependencies": {
    "phy-react": "latest",
    "typescript": "latest"
  },
  "devDependencies": {
    "@pandacss/dev": "latest",
    "@rollup/plugin-commonjs": "latest",
    "@rollup/plugin-node-resolve": "latest",
    "@types/react": "latest",
    "esbuild": "latest",
    "preact": "latest",
    "prettier": "latest",
    "rollup": "^3.26.2",
    "rollup-plugin-dts": "latest",
    "rollup-plugin-esbuild": "latest"
  },
  "peerDependencies": {
    "react": "latest"
  }
}
